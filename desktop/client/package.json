{
  "name": "client",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start": "ng serve --open",
    "format": "prettier --config ../.prettierrc.yaml --write \"src/**/*\"",
    "build": "ng build",
    "test": "ng test",
    "test:headless": "ng test --browsers ChromeHeadless --code-coverage --sourceMap=true --watch=false",
    "coverage": "ng test --code-coverage --sourceMap=true",
    "lint": "ng lint --type-check --format stylish",
    "lint:fix": "npm run lint -- --fix",
    "start:electron": "ng build --base-href ./ && electron .",
    "build:electron": "ng build --configuration production && electron-packager . --asar=true --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --out=./build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.2",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "animate.css": "^4.1.1",
    "material-components-web": "^10.0.0",
    "ngx-material-file-input": "^3.0.0",
    "rxjs": "~6.5.5",
    "socket.io-client": "^4.4.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.3",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@types/core-js": "^2.5.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^3.0.0",
    "angular": "^1.7.2",
    "angular-animate": "^1.7.2",
    "angular-aria": "^1.7.2",
    "angular-messages": "^1.7.2",
    "codelyzer": "^6.0.0",
    "electron": "^16.0.7",
    "electron-packager": "^15.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.1.6"
  }
}
